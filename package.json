{
  "name": "@pamanaleph/s3-presigned-upload",
  "version": "1.0.0",
  "description": "Simple TypeScript library for S3 presigned URL uploads with progress tracking and retry logic",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean",
    "dev": "tsup src/index.ts --dts --format esm --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test:run"
  },
  "keywords": [
    "s3",
    "presigned",
    "upload",
    "typescript",
    "browser",
    "progress",
    "retry",
    "xhr",
    "fetch"
  ],
  "author": "pamanaleph",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pamanaleph/s3-presigned-upload.git"
  },
  "bugs": {
    "url": "https://github.com/pamanaleph/s3-presigned-upload/issues"
  },
  "homepage": "https://github.com/pamanaleph/s3-presigned-upload#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^23.0.0",
    "msw": "^2.0.0",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}